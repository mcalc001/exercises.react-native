{
  "eslint.validate": [
      "javascript",
      "javascriptreact",
      "typescript",
      "typescriptreact"
  ],
  "eslint.alwaysShowStatus": true,
  "eslint.format.enable": true,
  "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
  },
  "editor.defaultFormatter": "dbaeumer.vscode-eslint",
  "editor.formatOnSave": true,

  "[typescriptreact]": {
      "editor.formatOnSave": false
  },
  "[javascript]": {
      "editor.formatOnSave": false
},
"cSpell.words": [
    "Pressable"
]
}